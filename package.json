{
  "name": "voice-relay-bot",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "start:batman": "node AudioRecorderBots/batmanRecorderAudio.js",
    "start:starfire": "node AudioRecorderBots/starfireRecorderAudio.js",
    "start:robin-1": "node AudioRelayBots/robinStreamAudioVersion-1.js",
    "start:robin-2": "node AudioRelayBots/robinStreamAudioVersion-2.js",
    "start:robin-3": "node AudioRelayBots/robinStreamAudioVersion-3.js",
    "start:robin-4": "node AudioRelayBots/robinStreamAudioVersion-4.js",
    "start:robin-5": "node AudioRelayBots/robinStreamAudioVersion-5.js",
    "register:batman": "node RegisterCommand/registerSlahCommandRecordBot.js",
    "register:robin-1": "node RegisterCommand/registerSlahCommandAudioStreamBot-Robin-1.js",
    "register:robin-2": "node RegisterCommand/registerSlahCommandAudioStreamBot-Robin-2.js",
    "register:robin-3": "node RegisterCommand/registerSlahCommandAudioStreamBot-Robin-3.js",
    "register:robin-4": "node RegisterCommand/registerSlahCommandAudioStreamBot-Robin-4.js",
    "register:robin-5": "node RegisterCommand/registerSlahCommandAudioStreamBot-Robin-5.js",
    "start:all": "concurrently \"npm:start:batman\" \"npm:start:starfire\" \"npm:start:robin-1\" \"npm:start:robin-2\" \"npm:start:robin-3\" \"npm:start:robin-4\" \"npm:start:robin-5\" ",
    "register:all":  "concurrently \"npm:register:batman\" \"npm:register:robin-1\" \"npm:register:robin-2\" \"npm:register:robin-3\" \"npm:register:robin-4\" \"npm:register:robin-5\" "
  },
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.18.0",
    "audio-mixer": "^2.1.4",
    "discord.js": "^14.18.0",
    "dotenv": "^16.5.0",
    "ffmpeg-static": "^5.2.0",
    "prism-media": "^1.3.5",
    "ws": "^8.18.1",
    "concurrently": "^8.2.2"
  }
}
